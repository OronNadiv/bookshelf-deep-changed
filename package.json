{
  "name": "bookshelf-deep-changed-plugin",
  "version": "1.0.3",
  "description": "Allows bookshelf models to check whether a value you save is different than the existing value in the database.",
  "main": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "bookshelf",
    "hasChanged",
    "deepChanged",
    "plugin",
    "compare values"
  ],
  "author": "Oron Nadiv",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/OronNadiv/bookshelf-deep-changed-plugin"
  },
  "bugs": {
    "url": "https://github.com/OronNadiv/bookshelf-deep-changed-plugin/issues"
  },
  "homepage": "https://github.com/OronNadiv/bookshelf-deep-changed-plugin",
  "engines": {
    "node": ">=4",
    "npm": ">=2"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && babel src/ --out-dir dist/",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "npm run build && nyc mocha  --require babel-core/register --recursive -t 10000 --exit",
    "test:coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "bluebird": ">=2 <4",
    "bookshelf": ">=0.7 <0.11",
    "knex": ">=0.7 <0.14",
    "pg": ">= 2.11.2 < 3.0.0 || >= 3.6.4 < 4.0.0 || >= 4.5.7 < 5.0.0 || >= 5.2.1 < 6.0.0 || >= 6.0.5 < 6.1.0 || >= 6.1.6 < 6.2.0 || >= 6.2.5 < 6.3.0 || >= 6.3.3 < 6.4.0 || >= 6.4.2 < 7.0.0 || >= 7.0.2 < 7.1.0 || ^7.1.2",
    "underscore": "^1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "chai": "^4.1.2",
    "chance": "^1.0.11",
    "coveralls": "^3.0.0",
    "eslint": "^4.8.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^4.0.1",
    "nyc": "^11.2.1",
    "rimraf": "^2.6.2"
  }
}
